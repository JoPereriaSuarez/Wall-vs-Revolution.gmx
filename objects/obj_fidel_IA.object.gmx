        <arguments>
          <argument>
            <kind>1</kind>
            <string>///COLLISION AND SPECIAL VELOCITY

if(dir != 0) {
    last_dir = dir;
}

//special velcity changes
if(!is_special)
{
    move_speed = normal_speed;
    hsp = dir * move_speed;
}
else{   
    if(dir != 0){
        hsp += dir * acce;
    }
    else
    {
        hsp -= last_dir * desacce;    
    }
    if(last_dir == 1)
    {
        hsp = max(0.000,hsp);
    }
    else{
        hsp = min(0.000,hsp);
    }
    hsp = clamp(hsp,-tank_speed,tank_speed);
}
