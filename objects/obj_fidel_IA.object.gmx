          <argument>
            <kind>1</kind>
            <string>///MOVEMENT
if(can_move)
{
    if(target &gt; 0)
    {
        var dist_to_target  = abs(target.x - x);
        var target_dir      = sign(target.x - x);
        var my_attack_distance = attack_distance;
//        show_debug_message(dist_to_target);
        if(instance_exists(obj_wall) &amp;&amp; target.id == obj_wall.id)
        {
            if(!is_special)
            {
                my_attack_distance = 95;  
            }
            else
            {
                //my_attack_distance = attack_distance + 106;            
                my_attack_distance = 5;
            }
        }
        if(dist_to_target &gt; my_attack_distance)
        {
            dir = target_dir;
        }
        else
        {
            dir = 0;
            if(is_special)
            {
                alarm[0]    = (time_attack/2) * room_speed;
                alarm[1]    = (time_move/2) * room_speed;
                if(!audio_is_playing(sfx_shooting_tank) &amp;&amp; !audio_is_playing(sfx_tanque_crear))
                {
                    audio_play_sound(sfx_shooting_tank,9,false);
                }
//                show_debug_message("ATTACK");
            }
            alarm[0]    = time_attack * room_speed;
            alarm[1]    = time_move * room_speed;
            can_attack  = false;
            can_move    = false;
            sprite_index = spr_attack;

        }
    }
    x += hsp;
}

</string>
